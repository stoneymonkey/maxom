<html><head>
<title>
Changes to transaction # <%= params[:id] %> of account <%= params[:acc] %>.
</title>
<meta http-equiv="Content-Type" content="text/html; charset=iso-8859-1"><script src="/javascripts/libForm.js"></script><script src="/javascripts/Add_CheckingAccountTrans.js"></script><LINK REL=STYLESHEET TYPE="text/css" HREF="/stylesheets/style.css"></head><% today = Date.today %><body bgcolor="#FFFFFF" onLoad="">

Making changes to deposit # <%= params[:id] %> of account <%= params[:acc] %>. <a href="javascript:delete_alert('<%= params[:acc] %>','<%= params[:id] %>')">Delete this transaction.</a>

<%= start_form_tag( { :action => "update", :acc => "#{params[:acc]}" }, { :name => "edit" }) %>

<%= hidden_field("acc", "", :value => "#{params[:acc]}") %>
<%= hidden_field("month", "", :value => "#{params[:month]}") %>
<%= hidden_field("year", "", :value => "#{params[:year]}") %>  <p>Adding a deposit to the check register. </p>  <table width="91%" border="1" height="279">    <tr>      <td width="53%">Bank Account: BOFW</td>      <td width="47%">&nbsp;</td>    </tr>    <tr>      <td width="53%">Date Received
        <%= text_field("Date Received", :value => "#{@item.Date_rec}", :size => "10") %>        <!-- input type="text" name="bofw_deposit[Date_rec]" value="<%= today.to_s %>" --> &nbsp;<img onClick="toggleDatePicker('daysOfMonth','new_cvcb_trans.cvcb[transaction_date]')" id="daysOfMonthPos" name="daysOfMonthPos" width=19 height=19 src="../images/calendaricon.gif" align=absmiddle border=0 alt="date picker"><div id="daysOfMonth" style="position:absolute;"></div>      </td>      <td width="47%">Client ID         <input type="text" name="bofw_deposit[Client_id]">      </td>    </tr>    <tr>      <td width="53%">Invoice ID       <input type="text" name="bofw_deposit[Invoice_id]" />      </td>      <td width="47%">Check Number         <input type="text" name="bofw_deposit[Chk_number]" />      </td>    </tr>    <tr>      <td width="53%">Amount Paid         <input type="text" name="bofw_deposit[Amount]" onChange="validate(document.forms[0].amount, document.forms[0].trans_type)">      </td>      <td width="47%">Visa         <input type="text" name="textfield" size="2" name="bofw_deposit[Visa]">        M/C         <input type="text" name="textfield2" size="2" name="bofw_deposit[Mastercard]">        Amex         <input type="text" name="textfield3" size="2" name="bofw_deposit[Amex]"></td>    </tr>    <tr>      <td width="53%"> Deposit Date         <input type="text" name="bofw_deposit[Deposit_date]">&nbsp;<img onClick="toggleDatePicker('daysOfMonth','new_cvcb_trans.cvcb[clear_date]')" id="daysOfMonthPos2" name="daysOfMonthPos2" width=19 height=19 src="../images/calendaricon.gif" align="middle" border=0 alt="date picker"><div id="daysOfMonthII" style="position:absolute;"></div>      </td>      <td width="47%">Tax Collected         <input type="text" name="bofw_deposit[Taxable]"></td>    </tr>    <tr>      <td width="53%">&nbsp; </td>      <td width="47%">&nbsp;</td>    </tr>    <tr>      <td width="53%">&nbsp;</td>      <td width="47%">&nbsp;</td>    </tr>    <tr>      <td width="53%">&nbsp;</td>      <td width="47%">&nbsp;</td>    </tr>  </table>  <input type="submit" name="Submit" value="Submit" /></form></body></html>